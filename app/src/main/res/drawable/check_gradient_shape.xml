<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="180"
                android:endColor="#29DEE5"
                android:startColor="#D46BE0" />
            <corners android:radius="@dimen/_20sdp" />
            <padding
                android:bottom="@dimen/_1sdp"
                android:left="@dimen/_1sdp"
                android:right="@dimen/_1sdp"
                android:top="@dimen/_1sdp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#2F2759" />
            <corners android:radius="@dimen/_20sdp" />
        </shape>
    </item>
</layer-list>